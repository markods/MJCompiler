program __class_member_2__

record Elem
{
    Elem next;
    int a;
}

class Stack
{
    Elem head;
    
    {
        Stack add( int a )
            Elem t;
        {
            t = new Elem;
            t.a = a;

            if( head == null ) { head = t; return; }
            t.next = head; head = t;

            return this;
        }

        bool isempty()
        {
            if( head == null ) return true;
            else               return false;
        }

        int top()
        {
            return head.a;
        }

        void pop()
        {
            if( head == null ) return;
            head = head.next;
        }
    }
}

{
    void main()
        Stack stack;
    {
        stack = new Stack;
        stack.add( 1 ).add( 2 ).add( 3 );

        do
        {
            if( stack.isempty() ) return;
            print( stack.top() ); print( ' ' );
            stack.pop();
        } while( true );
        print( eol );
    }
}
